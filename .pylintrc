# Generated by "pylint --generate-rcfile"
[MASTER]
ignore=CVS
ignore-patterns=
persistent=yes
unsafe-load-any-extension=no
load-plugins=
jobs=0
limit-inference-results=100
suggestion-mode=yes
auto-batch=no
py-version=3.11

[MESSAGES CONTROL]
disable=
    missing-module-docstring,
    missing-class-docstring,
    missing-function-docstring

[REPORTS]
output-format=text
reports=no
evaluation=10.0 - ((float(5 * error + warning + refactor + convention)) / statement)

[REFACTORING]
max-nested-blocks=5
never-returning-functions=sys.exit

[LOGGING]
logging-modules=logging

[SPELLING]
tmp-dir=

[TYPECHECK]
disabled-modules=
ignore-mixin-members=yes
ignored-classes=typing.NamedTuple
missing-member-hint=yes
missing-member-hint-distance=1
missing-member-max-choices=1
contextmanager-decorators=contextlib.contextmanager

[VARIABLES]
allowed-redefined-builtins=
dummy-variables-rgx=_+$
ignored-argument-names=_.*|^ignored_|^unused_
init-import=no

[BASIC]
argument-naming-style=snake_case
attr-naming-style=snake_case
class-attribute-naming-style=any
class-naming-style=PascalCase
const-naming-style=UPPER_CASE
function-naming-style=snake_case
good-names=i,j,k,ex,Run,_,id,pk
inlinevar-naming-style=any
method-naming-style=snake_case
module-naming-style=snake_case
no-docstring-rgx=^_
property-classes=abc.abstractproperty
variable-naming-style=snake_case

[FORMAT]
max-line-length=119
ignore-long-lines=^
max-module-lines=5000
indent-string='    '

[SIMILARITIES]
ignore-comments=yes
ignore-docstrings=yes
ignore-imports=yes
min-similarity-lines=4

[DESIGN]
max-args=5
max-attributes=10
max-bool-expr=5
max-branches=12
max-locals=15
max-parents=7
max-public-methods=20
max-returns=6
max-statements=50
min-public-methods=0

[CLASSES]
defining-attr-methods=__init__,__new__,setUp
exclude-protected=_asdict,_fields,_replace,_source
valid-classmethod-first-arg=cls
valid-metaclass-classmethod-first-arg=cls

[IMPORTS]
known-standard-library=
known-third-party=
known-first-party=server
allow-wildcard-with-all=no
analyse-fallback-blocks=no

[EXCEPTIONS]
overgeneral-exceptions=BaseException
